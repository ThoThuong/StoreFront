{
  "name": "storefront",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "npm run build && nodemon --exec ts-node ./src/server.ts",
    "build": "tsc --esModuleInterop src/server.ts --outDir ./dist",
    "start:prod": "npm run build && node dist/server.js",
    "test": "db-migrate --env test up && ENV=test ts-node node_modules/jasmine/bin/jasmine || db-migrate --env test down",
    "db-up": "db-migrate up",
    "db-down": "db-migrate down"
  },
  "author": "Tran Ngoc Thuong",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/humps": "^2.0.2",
    "@types/jasmine": "4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "humps": "^2.0.1",
    "jasmine": "4.1.0",
    "jasmine-core": "4.1.1",
    "jasmine-node": "^1.16.0",
    "jasmine-spec-reporter": "7.0.0",
    "jasmine-ts": "0.4.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.18",
    "supertest": "^6.2.4",
    "ts-node": "10.7.0",
    "tsc-node": "0.0.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "typescript": "^4.7.4"
  },
  "keywords": [],
  "description": ""
}